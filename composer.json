{
  "description": "BDD assertion library based on PHPUnit.",
  "homepage": "https://dev.belin.io/phpunit-expect",
  "license": "MIT",
  "name": "cedx/phpunit-expect",
  "type": "library",
  "authors": [{
    "email": "cedric@belin.io",
    "name": "CÃ©dric Belin"
  }],
  "autoload": {
    "files": ["lib/functions.php"],
    "psr-4": {"PHPUnit\\Expect\\": "lib/"}
  },
  "config": {
    "optimize-autoloader": true
  },
  "keywords": [
    "assert",
    "bdd",
    "expect",
    "phpunit",
    "test",
    "unit"
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-mbstring": "*",
    "ext-reflection": "*",
    "ext-spl": "*",
    "phpunit/phpunit": "^7.2.2"
  },
  "require-dev": {
    "cedx/coveralls": "^7.0.0",
    "phing/phing": "^2.16.1"
  },
  "scripts": {
    "coverage": "coveralls var/coverage.xml",
    "test": "phpunit --colors=auto --verbose"
  },
  "support": {
    "docs": "https://dev.belin.io/phpunit-expect/api",
    "issues": "https://github.com/cedx/phpunit-expect/issues"
  }
}
